<script type="text/javascript" src="@Url.Content("~/Scripts/Exportar/datatables.js")"></script>
<script src="@Url.Content("~/assets/js/jquery.table2excel.js")"></script>
<script src="@Url.Content("~/assets/js/tableToExcel.js")"></script>
